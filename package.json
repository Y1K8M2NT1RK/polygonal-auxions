  "engines": {
    "node": ">=20 <21",
    "npm": ">=10 <11"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "sh scripts/clean.sh",
    "build": "sh scripts/build.sh",
    "build:docker": "sh scripts/build.sh",
    "prisma:generate": "prisma generate && ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/fix-pothos-imports.ts",
    "build-with-debug": "NEXT_DEBUG=true npm run build",
  "vercel-build": "bash scripts/vercel-build.sh",
    "seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "seed:minimal": "SEED_MODE=minimal ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "start": "sh scripts/start.sh",
    "start:detached": "sh scripts/start_detached.sh",
    "stop": "sh scripts/stop.sh",
    "smoke": "sh scripts/smoke.sh",
    "docker:verify": "sh scripts/verify_once.sh",
    "lint": "next lint",
    "verify": "sh scripts/verify_compose.sh",
    "v": "npm run verify",
    "dc:build": "make dc-build",
    "db": "make dc-build",
    "docker:deps": "make docker-deps-install",
    "docker:deps:verify": "make docker-deps-verify",
    "docker:modules:reset": "make docker-reset-modules",
    "studio": "npx prisma studio --port 5555 --browser none",
    "email:test": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/email/simple-test.ts",
    "email:reset:test": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/dev-send-password-reset.ts",
    "email:raw-smtp": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/raw-smtp-send.ts"
  },
